92641"/>\n  <size package="105801" installed="225817" archive="226964"/>\n  <location href="Packages/fba2a2a6f6db89a6cb457b90d843cbba3d6e41b1e7b1de80c059cd601efe1303/v-hacd-tools-0:4.1.0-11.el10_0.x86_64.rpm"/>\n  <format>\n    <rpm:license>BSD-3-Clause AND MIT</rpm:license>\n    <rpm:vendor>Fedora Project</rpm:vendor>\n    <rpm:group>Unspecified</rpm:group>\n    <rpm:buildhost>buildvm-x86-07.iad2.fedoraproject.org</rpm:buildhost>\n    <rpm:sourcerpm>v-hacd-4.1.0-11.el10_0.src.rpm</rpm:sourcerpm>\n    <rpm:header-range start="4504" end="11953"/>\n    <rpm:provides>\n      <rpm:entry name="v-hacd-tools" flags="EQ" epoch="0" ver="4.1.0" rel="11.el10_0"/>\n      <rpm:entry name="v-hacd-tools(x86-64)" flags="EQ" epoch="0" ver="4.1.0" rel="11.el10_0"/>\n    </rpm:provides>\n    <rpm:requires>\n      <rpm:entry name="libgcc_s.so.1()(64bit)"/>\n      <rpm:entry name="libgcc_s.so.1(GCC_3.0)(64bit)"/>\n      <rpm:entry name="libgcc_s.so.1(GCC_3.3.1)(64bit)"/>\n      <rpm:entry name="libm.so.6()(64bit)"/>\n      <rpm:entry name="libm.so.6(GLIBC_2.2.5)(64bit)"/>\n      <rpm:entry name="libm.so.6(GLIBC_2.29)(64bit)"/>\n      <rpm:entry name="libm.so.6(GLIBC_2.38)(64bit)"/>\n      <rpm:entry name="libstdc++.so.6()(64bit)"/>\n      <rpm:entry name="libstdc++.so.6(CXXABI_1.3)(64bit)"/>\n      <rpm:entry name="libstdc++.so.6(CXXABI_1.3.11)(64bit)"/>\n      <rpm:entry name="libstdc++.so.6(CXXABI_1.3.13)(64bit)"/>\n      <rpm:entry name="libstdc++.so.6(CXXABI_1.3.15)(64bit)"/>\n      <rpm:entry name="libstdc++.so.6(CXXABI_1.3.2)(64bit)"/>\n      <rpm:entry name="libstdc++.so.6(CXXABI_1.3.3)(64bit)"/>\n      <rpm:entry name="libstdc++.so.6(GLIBCXX_3.4)(64bit)"/>\n      <rpm:entry name="libstdc++.so.6(GLIBCXX_3.4.11)(64bit)"/>\n      <rpm:entry name="libstdc++.so.6(GLIBCXX_3.4.14)(64bit)"/>\n      <rpm:entry name="libstdc++.so.6(GLIBCXX_3.4.15)(64bit)"/>\n      <rpm:entry name="libstdc++.so.6(GLIBCXX_3.4.18)(64bit)"/>\n      <rpm:entry name="libstdc++.so.6(GLIBCXX_3.4.19)(64bit)"/>\n  