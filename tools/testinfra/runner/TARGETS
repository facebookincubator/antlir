load("//antlir/bzl:oss_shim.bzl", "rust_binary", "third_party")

rust_binary(
    name = "runner",
    srcs = glob(["src/**/*.rs"]),
    deps = third_party.libraries(
        [
            "anyhow",
            "itertools",
            "rayon",
            "serde",
            "serde_json",
            "structopt",
        ],
        platform = "rust",
    ) + [third_party.library(
        "shed",
        "sql",
        platform = "rust",
    )],
)
