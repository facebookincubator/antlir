{
  "name": "doc-2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "yarn gen-pages && docusaurus start",
    "build": "yarn gen-pages && yarn docusaurus build",
    "gen-pages": "yarn bzldoc && yarn metalos-api",
    "metalos-api": "/bin/bash -c 'buck run //antlir/website/gen:generate-metalos-apis > docs/metalos/apis.md'",
    "bzldoc": "node ./bzldoc.js --files='//antlir/bzl:image.bzl //antlir/bzl:shape.bzl //antlir/bzl/image/feature:defs.bzl //antlir/bzl/image/package:defs.bzl //antlir/vm/bzl:defs.bzl //antlir/bzl:flavor_helpers.bzl //antlir/bzl:test_rpms.bzl'",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "ci": "yarn lint && yarn prettier:diff",
    "lint": "eslint --cache \"**/*.js\" && stylelint \"**/*.css\"",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,md}\"",
    "prettier:diff": "prettier --config .prettierrc --list-different \"**/*.{js,md}\""
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.21",
    "@docusaurus/preset-classic": "2.0.0-beta.21",
    "clsx": "^1.1.1",
    "docusaurus-plugin-internaldocs-fb": "0.12.2",
    "mdx-mermaid": "^1.2.2",
    "mermaid": "^9.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "prettier": "^2.0.2",
    "stylelint": "^13.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "**/node-fetch": "2.6.1",
    "**/highlight.js": "10.4.1",
    "**/immer": "8.0.1",
    "shelljs": "^0.8.5",
    "ansi-html": "0.0.8"
  }
}
