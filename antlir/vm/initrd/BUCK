load("//antlir/bzl:oss_shim.bzl", "export_file", "kernel_get")

[export_file(
    name = f,
    visibility = [
        kernel_get.base_target + "/...",
        "//metalos/...",
        "//scripts/dschatzberg/vm:",
    ],
) for f in glob([
    "*.service",
    "*.mount",
    "*.conf",
])]
